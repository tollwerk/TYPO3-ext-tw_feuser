    <file source-language="en" datatype="plaintext" original="messages" date="2019-04-17T10:00:00Z"
          product-name="tw_user">
        <header/>
        <body>
            <trans-unit id="feuser.registration.form.email" approved="yes">
                <source>Email</source>
                <target state="signed-off">E-mail</target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.firstName" approved="yes">
                <source>First name</source>
                <target state="translated">Voornaam
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.lastName" approved="yes">
                <source>Last name</source>
                <target state="signed-off">Achternaam
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.email.placeholder" approved="yes">
                <source>john@doe.com</source>
                <target state="translated">jan@jansen.com
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.terms.label" approved="yes">
                <source>Terms and privacy disclaimer</source>
                <target state="translated">Erkenning van de verklaring inzake gegevensbescherming en de
                    huurvoorwaarden
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.password" approved="yes">
                <source>Password</source>
                <target state="translated">Wachtwoord</target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.repeatPassword" approved="yes">
                <source>Repeat password</source>
                <target state="translated">Wachtwoord herhalen</target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.terms" approved="yes">
                <source>
                    <![CDATA[I hereby confirm that I have read and understood your <a href="%s" target="_blank">Terms & Conditions</a> and your <a href="%s" target="_blank">Privacy Policy</a> and that I agree to both.]]>
                </source>
                <target state="translated">
                    <![CDATA[Ik bevestig dat ik de <a href="%s" target="_blank">informatie over de algemene voorwaarden</a> en de <a href="%s" target="_blank">privacy</a> heb gelezen en ermee akkoord ga.]]>
                </target>
            </trans-unit>

            <trans-unit id="feuser.registration.form.submit" approved="yes">
                <source>Register now</source>
                <target state="signed-off">Registreer nu
                </target>
            </trans-unit>
            <trans-unit id="feuser.profile.form.submit">
                <source>Save changes</source>
                <target></target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.submitted.title" approved="yes">
                <source>Thank you for registering</source>
                <target state="translated">Bedankt voor uw inschrijving
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.submitted.message" approved="yes">
                <source>We have sent you an email. Please click on the confirmation link inside to finish your
                    registration.
                </source>
                <target state="translated">We hebben u een e-mailbericht gestuurd. Ga naar de bevestigings-URL in het
                    bericht om uw registratie af te ronden.
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.success.title" approved="yes">
                <source>Registration confirmed</source>
                <target state="translated">Registratie Bevestigd
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.success.message" approved="yes">
                <source>Thank you for confirming your registration. You can now sign in to the website.</source>
                <target state="translated">Hartelijk dank voor uw registratie. U kunt nu inloggen op de website.
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.error.title" approved="yes">
                <source>Registration error</source>
                <target state="translated">Fout tijdens de registratie
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.error.message" approved="yes">
                <source>We are sorry but there was an error. Please contact us directly if this keeps happening.
                </source>
                <target state="translated">Het spijt ons, maar er is een fout gemaakt. Probeer het opnieuw en neem
                    direct contact met ons op als de fout blijft bestaan.
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.email.subject" approved="yes">
                <source>Please confirm your registration</source>
                <target state="translated">Gelieve uw registratie te bevestigen
                </target>
            </trans-unit>
            <trans-unit id="feuser.registration.email.body.html" approved="yes">
                <source>&lt;p&gt;Thank your for registering. Please confirm your registration by clicking on the
                    following link:&lt;/p&gt; &lt;blockquote&gt; &lt;a href="%s" target="_blank"&gt;%s&lt;/a&gt; &lt;/blockquote&gt;
                    &lt;p&gt;After that you can sign in with the following credentials:&lt;/p&gt; &lt;blockquote&gt;
                    &lt;strong&gt;Username:&lt;/strong&gt; %s&lt;br /&gt; &lt;strong&gt;Password:&lt;/strong&gt; %s &lt;/blockquote&gt;
                    &lt;p&gt;If you do not wish to register anymore you can simply ignore this message.&lt;/p&gt;
                </source>
                <target state="translated"></target>
            </trans-unit>
            <trans-unit id="feuser.registration.email.body.plaintext" approved="yes">
                <source>Please confirm your registration by clicking on the following link: %s After that you can sign
                    in with the following credentials: Username: %s Password: %s If you do not wish to register anymore
                    you can simply ignore this message.
                </source>
                <target state="translated"></target>
            </trans-unit>

            <trans-unit id="feuser.registration.form.email.error" approved="yes">
                <source>Email address already registered</source>
                <target state="translated">Reeds toegewezen e-mailadres
                </target>
            </trans-unit>
        </body>
    </file>
