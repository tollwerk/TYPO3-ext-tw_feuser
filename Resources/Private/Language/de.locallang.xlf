<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<xliff version="1.0">
    <file source-language="en" datatype="plaintext" original="messages" date="2019-04-17T10:00:00Z"
          product-name="tw_user">
        <header/>
        <body>
            <!-- Registration form -->
            <trans-unit id="feuser.registration.form.email">
                <source>Email</source>
                <target>E-Mail</target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.email.placeholder">
                <source>john@doe.com</source>
                <target>max@mustermann.de</target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.email.error.unique">
                <source>The email address is already taken</source>
                <target>Diese Adresse ist bereits vergeben</target>
            </trans-unit>
            <trans-unit id="feuser.registration.form.submit">
                <source>Register now</source>
                <target>Jetzt registrieren</target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.submitted.title">
                <source>Thank you for registering</source>
                <target>Danke für Ihre Registrierung</target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.submitted.message">
                <source>We have sent you an email. Please click on the confirmation link inside to finish your registration.</source>
                <target>Sie haben eine E-Mail erhalten. Bitte klicken Sie dort auf den Bestätigungslink, um Ihre Registrierung abzuschließen.</target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.success.title">
                <source>Registration confirmed</source>
                <target>Registrierung abgeschlossen</target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.success.message">
                <source>Thank you for confirming your registration. You can now sign in to the website.</source>
                <target>Vielen Dank, Sie haben Ihre Registrierung hiermit abgeschlossen und können sich nun mit Ihren Zugangsdaten anmelden.</target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.error.title">
                <source>Registration error</source>
                <target>Fehler bei der Registrierung</target>
            </trans-unit>
            <trans-unit id="feuser.registration.status.error.message">
                <source>We are sorry but there was an error. Please contact us directly if this keeps happening.</source>
                <target>Es tut uns leid, aber bei Ihrer Registrierung ist ein Fehler aufgetreten. Bitte versuchen Sie es noch einmal oder kontaktieren Sie uns direkt. Vielen Dank für Ihr Verständnis.</target>
            </trans-unit>
            <trans-unit id="feuser.registration.adminEmail.subject">
                <source>A new user signed up</source>
                <target>Ein neuer Benutzer hat sich registriert</target>
            </trans-unit>
            <trans-unit id="feuser.registration.email.subject">
                <source>Please confirm your registration</source>
                <target>Bitte bestätigen Sie Ihre Registrierung</target>
            </trans-unit>
            <trans-unit id="feuser.registration.email.body.html">
                <source><![CDATA[
                    <p>Thank your for registering. Please confirm your registration by clicking on the following link:</p>
                    <blockquote>
                        <a href="%s" target="_blank">%s</a>
                    </blockquote>
                    <p>After that you can sign in with the following credentials:</p>
                    <blockquote>
                        <strong>Username:</strong> %s<br />
                        <strong>Password:</strong> %s
                    </blockquote>
                     <p>If you do not wish to register anymore you can simply ignore this message.</p>
                ]]></source>
                <target><![CDATA[
                    <p>Danke für Ihre Registrierung. Um diese abzuschließen, klicken Sie bitte auf den folgenden Link:</p>
                    <blockquote>
                        <a href="%s" data-href="%s" target="_blank">Registrierung abschließen</a>
                    </blockquote>
                    <p>Danach können Sie sich mit folgenden Zugangsdaten anmelden:</p>
                    <blockquote>
                        <strong>Benutzername:</strong> %s<br />
                        <strong>Passwort:</strong> %s
                    </blockquote>
                     <p>Wenn Sie sich doch nicht mehr registrieren möchten, können Sie diese E-Mail einfach ignorieren.</p>
                ]]></target>
            </trans-unit>
            <trans-unit id="feuser.registration.email.body.plaintext">
                <source>
                    Please confirm your registration by clicking on the following link:

                    %s

                    After that you can sign in with the following credentials:

                    Username: %s
                    Password: %s

                    If you do not wish to register anymore you can simply ignore this message.
                </source>
                <target>
Danke für Ihre Registrierung. Um diese abzuschließen, klicken Sie bitte auf den folgenden Link:

%s

Danach können Sie sich mit folgenden Zugangsdaten anmelden:
Benutzername: %s
Passwort: %s

Wenn Sie sich doch nicht mehr registrieren möchten, können Sie diese E-Mail einfach ignorieren.
                </target>
            </trans-unit>

            <!-- Profile update form -->
            <trans-unit id="feuser.profile.form.submit">
                <source>Update</source>
                <target>Aktualisieren</target>
            </trans-unit>
            <trans-unit id="feuser.profile.form.givenName">
                <source>Given name</source>
                <target>Vorname</target>
            </trans-unit>
            <trans-unit id="feuser.profile.form.givenName.placeholder">
                <source>John</source>
                <target>Max</target>
            </trans-unit>
            <trans-unit id="feuser.profile.form.familyName">
                <source>Family name</source>
                <target>Nachname</target>
            </trans-unit>
            <trans-unit id="feuser.profile.form.familyName.placeholder">
                <source>Doe</source>
                <target>Mustermann</target>
            </trans-unit>
            <trans-unit id="feuser.profile.status.update.success.message">
                <source>Profile updated</source>
                <target>Profil aktualisiert</target>
            </trans-unit>
            <trans-unit id="feuser.profile.status.update.success.message">
                <source>Your successfully updated your profile.</source>
                <target>Sie haben Ihr Profil erfolgreich aktualisiert.</target>
            </trans-unit>
            <trans-unit id="feuser.profile.status.update.error.title">
                <source>Sorry but there was an error updating your profile.</source>
                <target>Entschuldigung, aber Ihr Profil konnte nicht erfolgreich aktualisiert werden.</target>
            </trans-unit>
            <trans-unit id="feuser.profile.update.adminEmail.subject">
                <source>A user updated his profile</source>
                <target>Ein Benutzer hat seine Konto aktualisiert</target>
            </trans-unit>

            <!-- Change password -->
            <trans-unit id="feuser.password.form.submit">
                <source>Change password</source>
                <target>Passwort ändern</target>
            </trans-unit>
            <trans-unit id="feuser.password.form.password">
                <source>Password</source>
                <target>Passwort</target>
            </trans-unit>
            <trans-unit id="feuser.password.form.password.placeholder">
                <source>Your new password</source>
                <target>Ihr neues Passwort</target>
            </trans-unit>
            <trans-unit id="feuser.password.status.success">
                <source>You successfully changed your password</source>
                <target>Sie haben Ihr Passwort erfolgreich geändert</target>
            </trans-unit>
            <trans-unit id="feuser.password.status.error">
                <source>Sorry, but there was an error changing your password</source>
                <target>Entschuldigung, aber Ihr Passwort konnte nicht geändert werden</target>
            </trans-unit>
            <trans-unit id="feuser.password.form.confirmPassword">
                <source>Repeat password</source>
                <target>Passwort wiederholen</target>
            </trans-unit>

            <!-- Validators -->
            <trans-unit id="validator.password.minLength.error">
                <source>Your password is too short. Please use at least %s characters.</source>
                <target>Ihr Passwort ist zu kurz. Bitte nutzen Sie mindestens %s Zeichen.</target>
            </trans-unit>
        </body>
    </file>
